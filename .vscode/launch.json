{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch C++ App",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/ConsoleTester/Main",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "build",
            "setupCommands": 
            [
                {
                    "description": "Enable pretty printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}